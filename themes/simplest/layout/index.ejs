<h2><%= config.title %></h2>
<p><a href="/en">English</a></p>
<h3>原创：</h3>
<ul class="idx">
  <% page.posts.each(function(post){ %>
  <% var tag = {}; post.tags.each(function(c) { tag[c.name] = 1 }) %>
  <% if (!("zz" in tag)) { %>
  <li><a href="<%- config.root %><%- post.path%>"><%= post.title %></a></li>
  <% } %>
  <% }) %>
</ul>
<h3>转载：</h3>
<ul class="idx">
  <% page.posts.each(function(post){ %>
  <% var tag = {}; post.tags.each(function(c) { tag[c.name] = 1 }) %>
  <% if ("zz" in tag) { %>
  <li><a href="<%- config.root %><%- post.path%>"><%= post.title %></a></li>
  <% } %>
  <% }) %>
</ul>

